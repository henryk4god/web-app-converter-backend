{
    "version": 2,
    "builds": [
      {
        "src": "api/index.py",
        "use": "@vercel/python",
        "config": {
          "runtime": "python3.9",
          "maxLambdaSize": "50mb",
          "includeFiles": [
            "api/apk_generator.py",
            "api/payment.py",
            ".env"
          ]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "api/index.py"
      }
    ]
  }